<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://axelor.com/xml/ns/object-views
  http://axelor.com/xml/ns/object-views/object-views_5.0.xsd">

	<grid name="accounting-grid" title="Accounting"
		model="com.axelor.accounting.db.Accounting">
		<field name="accountingLine" />
		<field name="dateOfAccounting" />
		<field name="referenceAccountingDoc" />
	</grid>

	<form name="accounting-form" title="Accounting"
		model="com.axelor.accounting.db.Accounting" width="large">
		<panel>
			<field name="accountingLine" />
			<field name="dateOfAccounting" />
			<field name="referenceAccountingDoc" />
		</panel>
	</form>

</object-views>

