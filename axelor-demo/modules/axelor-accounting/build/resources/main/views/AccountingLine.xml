	xsi:schemaLocation="http://axelor.com/xml/ns/object-views
  http://axelor.com/xml/ns/object-views/object-views_5.0.xsd">

	<grid name="accounting-line-grid" title="Accounting Lines"
		model="com.axelor.accounting.db.AccountingLine">
		<field name="accountingAccount" />
		<field name="debit" />
		<field name="credit" />

	</grid>

	<form name="accounting-line-form" title="Accounting Lines"
