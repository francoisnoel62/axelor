<?xml version="1.0" encoding="UTF-8"?>
<domain-models
	xmlns="http://axelor.com/xml/ns/domain-models"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://axelor.com/xml/ns/domain-models
  http://axelor.com/xml/ns/domain-models/domain-models_5.0.xsd">

	<module name="sale2" package="com.axelor.sale2.db" />

	<entity name="SaleOrder2" sequential="true" cachable="true">
		<many-to-one name="customer"
			ref="com.axelor.contact.db.Contact" />

		<date name="dateOfOrder" default="now" />
		
		<date name="dateOfPrevInvoicing" />
		
		<one-to-many name="saleOrderLine2" ref="SaleOrderLine2"
			mappedBy="saleOrder2" />
			
		<decimal name="exTaxTotal" />
		
		<decimal name="inTaxTotal" />

	</entity>
</domain-models>