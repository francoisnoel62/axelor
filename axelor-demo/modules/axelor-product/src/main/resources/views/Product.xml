<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://axelor.com/xml/ns/object-views
  http://axelor.com/xml/ns/object-views/object-views_5.0.xsd">

	<grid name="product-grid" title="Products" model="com.axelor.product.db.Product">
		<field name="name" />
		<field name="productFamily" />
		<field name="description" />
		<field name="exTaxPrice" />
		<field name="accountingAccount" />
	</grid>

	<form name="product-form" title="Product" model="com.axelor.product.db.Product">
		<panel>
			<field name="name" />
			<field name="productFamily" />
			<field name="description" />
			<field name="exTaxPrice" />
			<field name="accountingAccount" />
		</panel>
	</form>

</object-views>

